{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "PDF Rancher",
  "version": "0.1.0",
  "identifier": "ca.wako.pdf-editor",
  "build": {
    "frontendDist": "..",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "PDF Rancher"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
